y=t\\
z=-t
\end{matrix}\Rightarrow\bigg\{\begin{matrix}x=1\\
y=-z
\end{matrix}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Reemplazo en la ecuación de }S\rightarrow\mathbb{L}_{1}:1+z^{2}-z^{2}=1\wedge\mathbb{L}_{2}:1+\left(-z\right)^{2}-z^{2}=1
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore\mathbb{L}_{1}\subset S\wedge\mathbb{L}_{2}\subset S
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f(1,t,-t)=1+t^{2}-(-t)^{2}=1\forall t\therefore L_{2}\in S
