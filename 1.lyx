\end_inset

 se cumpla que 
\begin_inset Formula $3<a_{n}<5$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\lim_{n\to\infty}a_{n}=\lim_{n\to\infty}\frac{4n-10}{n+1}=4\rightarrow\mbox{por el criterio de límite tendiendo a infinito de cociente de polinomios}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Rightarrow\forall\varepsilon>0,\exists n_{0}>0:\forall n>n_{0},|\frac{4n-10}{n+1}-4|<\varepsilon\rightarrow\mbox{Por definición de límite}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Si }\varepsilon=1\Rightarrow\exists n_{0}>0:\forall n>n_{0},|\frac{4n-10}{n+1}-4|<1\Rightarrow3<a_{n}<5\rightarrow\mbox{ por definición de distancia}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left|\frac{4n-10}{n+1}-\frac{4n+4}{n+1}\right|<1\Leftrightarrow\left|-\frac{14}{n+1}\right|<1\Leftrightarrow\frac{14}{n+1}<1\Leftrightarrow14-1<n
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore\forall n>13,|\frac{4n-10}{n+1}-4|<1\Rightarrow n_{0}=13
\]

\end_inset


\end_layout

\begin_layout Subsection
Encontrar, si existen, 
\begin_inset Formula $max\{a_{n}:n\in\mathbb{N}\}$
\end_inset

 y 
\begin_inset Formula $min\{a_{n}:n\in\mathbb{N}\}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
a_{n+1}>a_{n}\Leftrightarrow\frac{4(n+1)-10}{n+2}>\frac{4n-10}{n+1}\Leftrightarrow(4n+4-10)(n+1)>(n+2)(4n-10)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Leftrightarrow(4n-6)(n+1)>(n+2)(4n-10)\Leftrightarrow(4n-6)(n+1)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Leftrightarrow4n^{2}-6n+4n-6>4n^{2}-10n+8n-20\Leftrightarrow-2n+2n-6+20>0\Leftrightarrow14>0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore a_{n}\mbox{ es monótona estrictamente creciente\therefore\ensuremath{min\{a_{n}:n\in\mathbb{N}\}=a_{1}=\frac{4-10}{2}=-3}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
sup\{a_{n}:n\in\mathbb{N}\}=\lim_{n\to\infty}a_{n}=4\rightarrow\mbox{ por ser convergente y monótona creciente}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Como es creciente: }\nexists a_{n}/a_{n}=4\rightarrow\mbox{porque entonces para los siguientes números se alejaría del 4 y no sería convergente, como se probó.}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore4\notin\left(a_{n}\right)\Rightarrow\nexists max\{a_{n}:n\in\mathbb{N}\}
\]

\end_inset


\end_layout

\begin_layout Section
Sean 
