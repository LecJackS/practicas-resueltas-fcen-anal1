\begin_layout Standard
Probar que:
\end_layout

\begin_layout Subsection
\begin_inset Formula $\lim_{n\to\infty}a_{n}=+\infty$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Defino }a'_{n}=\frac{n+1}{\sqrt{2n}}\leq a_{n}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\lim_{n\to\infty}a'_{n}=\lim_{n\to\infty}\frac{n+1}{\sqrt{2n}}=\lim_{n\to\infty}\sqrt{\frac{(n+1)^{2}}{2n}}=\lim_{n\to\infty}\sqrt{\frac{n}{2}+\frac{1}{2n}+1}=\sqrt{+\infty}=+\infty
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
a'_{n}\leq a_{n}\Rightarrow\lim_{n\to\infty}a_{n}=+\infty
\]

\end_inset


\end_layout

\begin_layout Subsection
\begin_inset Formula $\lim_{n\to\infty}b_{n}=1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Defino }b'_{n}=\frac{n+1}{n}\geq b_{n}\mbox{ y }b''_{n}=\frac{n+1}{\sqrt{n^{2}+n}}\leq b_{n}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\lim_{n\to\infty}b'_{n}=\lim_{n\to\infty}\frac{n+1}{n}=\lim_{n\to\infty}1+\frac{1}{n}=1
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\lim_{n\to\infty}b''_{n}=\lim_{n\to\infty}\frac{n+1}{\sqrt{n^{2}+n}}=\lim_{n\to\infty}\sqrt{\frac{(n+1)^{2}}{n^{2}+n}}=\lim_{n\to\infty}\sqrt{\frac{n^{2}+n+n+1}{n^{2}+n}}=\lim_{n\to\infty}\sqrt{\frac{n+1}{n^{2}+n}+1}=\lim_{n\to\infty}\sqrt{\frac{n+1}{n(n+1)}+1}=\lim_{n\to\infty}\sqrt{\frac{1}{n}+1}=\sqrt{1}=1
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
b'_{n}\geq b_{n}\geq b''_{n}\wedge\lim_{n\to\infty}b''_{n}=\lim_{n\to\infty}b''_{n}=1\Rightarrow\lim_{n\to\infty}b_{n}=1
\]

\end_inset


\end_layout

\begin_layout Section
Sea 
\begin_inset Formula $a_{n}=\frac{4n-10}{n+1}$
