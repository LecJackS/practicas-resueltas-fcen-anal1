\begin_inset Formula $\lim_{n\to\infty}\frac{2n}{7n-3}=\lim_{n\to\infty}\frac{2n}{7n}$
\end_inset

 ya que es un cociente de polinomios
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
A_{n+1}/A_{n}=\frac{2n+2}{7n+7-3}*\frac{7n-3}{2n}=\frac{14n^{2}+14n-6n-6}{14n^{2}+8n}=\frac{14n^{2}+8n-6}{14n^{2}+8n}=1-\frac{6}{14n^{2}+8n}>0\forall n\in\mathbb{N}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Rightarrow\left(A_{n}\right)\mbox{ es creciente }\therefore A_{1}=\frac{1}{2}\mbox{ es el mínimo}\wedge\frac{2}{7}\mbox{ es el supremo, como nunca lo alcanza no tiene máximo.}
\]

\end_inset


\end_layout

\begin_layout Section
Resolver
\end_layout

\begin_layout Subsection
Considerar el conjunto 
\begin_inset Formula $A=\{a\in\mathbb{Q}:a^{2}<2\}\subset\mathbb{R}.$
\end_inset

 Calcule su supremo y concluya que 
\begin_inset Formula $A$
\end_inset

 no tiene máximo.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
a\in A\Leftrightarrow a^{2}<2\Leftrightarrow|a|<\sqrt{2}\Rightarrow\mbox{supremo de }A=\sqrt{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sqrt{2}\notin\mathbb{Q}\Rightarrow A\mbox{ no tiene máximo.}
\]

\end_inset


\end_layout

\begin_layout Subsection
Dado el conjunto 
\begin_inset Formula $B=\{a\in\mathbb{Q}_{>0}:a^{2}>2\}\subset\mathbb{R}$
\end_inset

, calcule su ínfimo y concluya que B no tiene mínimo.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Por el punto anterior y la def. de \ensuremath{B}: }0<a<\sqrt{2}\Rightarrow\mbox{ínfimo de }B=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
0\notin A\Rightarrow A\mbox{ no tiene mínimo.}
\]

\end_inset


\end_layout

\begin_layout Section
Calcular
\end_layout

\begin_layout Subsection
\begin_inset Formula $sup\left\{ \frac{n^{2}}{2^{n}}:n\in\mathbb{N}\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left(A_{n}\right)=\left\{ \frac{1}{2},1,\frac{9}{8},1,\frac{25}{32},\frac{36}{64},\frac{49}{128},\frac{64}{256}=\frac{1}{4},...\right\} 
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\forall n\in\mathbb{N}:A_{n}=\frac{n^{2}}{2^{n}}\wedge A_{n+1}=\frac{\left(n+1\right)^{2}}{2.2^{n}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{(n+1)^{2}}{2^{n+1}}*\frac{2^{n}}{n^{2}}=\frac{(n+1)^{2}}{2*n^{2}}=\frac{1}{2}+\frac{2n+1}{2n^{2}}=\frac{1}{2}+\frac{1}{n}+\frac{1}{2n^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{1}{2}+\frac{1}{n}+\frac{1}{2n^{2}}<1\Leftrightarrow\frac{1}{n}+\frac{1}{2n^{2}}<\frac{1}{2}\Leftrightarrow n>3
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore\forall n>3:\left(A_{n}\right)\mbox{ es decreciente}\Rightarrow sup\left\{ \frac{n^{2}}{2^{n}}:n\in\mathbb{N}\right\} =\frac{9}{8}
\]

\end_inset


\end_layout

\begin_layout Subsection
\begin_inset Formula $sup\left\{ \frac{\sqrt{n+1}}{10+n}:n\in\mathbb{N}\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left(A_{n}\right)=\left\{ \frac{\sqrt{2}}{11},\frac{\sqrt{3}}{12},\frac{2}{13},...\right\} 
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\sqrt{n+1}}{10+n}=\sqrt{\left(\frac{\sqrt{n+1}}{10+n}\right)^{2}}=\sqrt{\frac{n+1}{20(n+1)+81-1+n^{2}}}=\sqrt{\frac{n+1}{(n+1)*(20+\frac{81}{n+1}+\frac{n^{2}-1}{n+1})}}=\sqrt{\frac{1}{20+\frac{81}{n+1}+n-1}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\lim_{n\to\infty}A_{n}=\lim_{n\to\infty}\sqrt{\frac{1}{20+\frac{81}{n+1}+n-1}}=0\Rightarrow\frac{\sqrt{n+1}}{10+n}\mbox{es convergente\ensuremath{\therefore}es acotada\ensuremath{\therefore}tiene supremo e ínfimo}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{A_{n+1}}{A_{n}}=\frac{\sqrt{20+\frac{81}{n+1}+n-1}}{\sqrt{20+\frac{81}{n+2}+n}}=\sqrt{\frac{20+\frac{81}{n+1}-\frac{n+1}{n+1}+n}{20+\frac{81}{n+2}+n}}=\sqrt{\frac{20+\frac{80-n}{n+1}+n}{20+\frac{81}{n+2}+n}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sqrt{\frac{20+\frac{80-n}{n+1}+n}{20+\frac{81}{n+2}+n}}<1\Leftrightarrow\frac{80-n}{n+1}<\frac{81}{n+2}\Rightarrow\mbox{Resuelvo la inecuación }\frac{80-n}{n+1}<\frac{81}{n+2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
(n+2)(80-n)<(n+1)81\Leftrightarrow80n+160-n^{2}-2n<81n+81\Leftrightarrow-n^{2}+78n+160-81-81n<0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Leftrightarrow-n^{2}-3n+79<0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Pruebo algunos números: }\begin{cases}
n=7 & -49-21+79=9\\
n=8 & -64-24+79=-9
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore\frac{\sqrt{n+1}}{10+n}\mbox{ es creciente hasta 7 y luego decrece, convergiendo a 0}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore A_{7}\mbox{ es el supremo}=\frac{\sqrt{8}}{17}=\frac{2\sqrt{2}}{17}
\]

\end_inset


\end_layout

\begin_layout Subsection
inf
\begin_inset Formula $\left\{ \frac{n-3}{2^{n}}:n\in\mathbb{N}\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
A_{n}=\frac{n-3}{2^{n}};\lim_{n\to\infty}A_{n}=\lim_{n\to\infty}\frac{n-3}{2^{n}}=0\therefore\mbox{es convergente\ensuremath{\therefore}es acotada\ensuremath{\therefore}tiene supremo e ínfimo}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{A_{n+1}}{A_{n}}=\frac{n-2}{2^{n+1}}*\frac{2^{n}}{n-3}=\frac{n-2}{2n-6}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{n-2}{2n-6}>1\Leftrightarrow n-2>2n-6\Leftrightarrow n<4
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore\frac{n-3}{2^{n}}\mbox{ es creciente hasta }n=4\mbox{ y luego decrece convergiendo a 0}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore\mbox{ inf}\left\{ \frac{n-3}{2^{n}}:n\in\mathbb{N}\right\} =min(A_{1},A_{2},A_{3},0)=min(\frac{-2}{2},\frac{-1}{4},\frac{0}{8},0)=-1
\]

\end_inset


\end_layout

\begin_layout Subsection
inf
\begin_inset Formula $\left\{ n^{2}-9n-10:n\in\text{\mathbb{N}}\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
A_{n}=n^{2}-9n-10
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Como es un polinomio (continuo con n\in\mathbb{R}) uso el criterio de la derivada para hayar máximos y mínimos:}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
A_{n}'=2n-9=0\Leftrightarrow n=\frac{9}{2}\Rightarrow\mbox{reviso los valores más cercanos }\in\mathbb{N}:\begin{cases}
n=4 & 16-36-10=-30\\
n=5 & 25-45-10=-30
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
A_{n}''=2\therefore n=4\wedge n=5\mbox{ son mínimos locales}\therefore\mbox{inf}\left\{ n^{2}-9n-10:n\in\text{\ensuremath{\mathbb{N}}}\right\} =A_{4/5}=-30
\]

\end_inset


\end_layout

