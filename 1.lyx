\begin_layout Subsection
\begin_inset Formula $a<a^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{cases}
a<0 & a\leq|a|\leq a^{2}\\
0\leq a\leq1 & a^{2}=a.a\leq a\rightarrow\mbox{ Ya que a reduce a cualquier número, en particular a }a\\
a>1 & a^{2}>a\rightarrow\mbox{ Ya que a incrementa a cualquier número, en particular a }a
\end{cases}\therefore a<a^{2}\Leftrightarrow a>1
\]

\end_inset


\end_layout

\begin_layout Subsection
\begin_inset Formula $a<b\Rightarrow a^{2}<b^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{cases}
a>1\wedge a<b\Rightarrow & a*a<b*a\Rightarrow a^{2}<b*a<b*b\Rightarrow a^{2}<b^{2}\\
0\leq a\leq1\wedge a<b\Rightarrow & a*a<b*a\Rightarrow a^{2}<b*a<b*b\Rightarrow a^{2}<b^{2}\\
a<0\wedge|a|<|b|\wedge a<b\Rightarrow & |a|*|a|<|b|*|a|\Rightarrow a^{2}<|b|*|a|<|b|*|b|\Rightarrow a^{2}<b^{2}\\
a<0\wedge|a|\geq|b|\wedge a<b\Rightarrow & |a|*|a|\geq|b|*|a|\Rightarrow a^{2}\geq|b|*|a|\geq|b|*|b|\Rightarrow a^{2}\geq b^{2}
\end{cases}\therefore a<b\Rightarrow a^{2}<b^{2}\Leftrightarrow a<0\wedge|a|\geq|b|
\]

\end_inset


\end_layout

