\begin_layout Standard
\begin_inset Formula 
\[
a_{n}=\frac{1}{\sqrt{n}}+\frac{1}{\sqrt{n+1}}+...+\frac{1}{\sqrt{2n}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
b_{n}=\frac{1}{\sqrt{n^{2}}}+\frac{1}{\sqrt{n^{2}+1}}+...+\frac{1}{\sqrt{n^{2}+n}}
\]

